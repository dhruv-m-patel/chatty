{
  "name": "high-five-service",
  "version": "0.0.7",
  "main": "build/index.js",
  "scripts": {
    "seed": "ts-node -r dotenv/config src/seed.ts dotenv_config_path=../../.env",
    "build": "rimraf ./build && tsc && copyfiles -u 1 src/api/* build",
    "start": "node -r dotenv/config build/index.js",
    "start-dev": "DEBUG=swagger-express-validator nodemon",
    "test": "NODE_ENV=test jest --runInBand --silent",
    "test:ci": "NODE_ENV=test jest --ci --coverage --coverageReporters=text --coverageReporters=text-summary ",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@alcumus/express-app": "^0.0.6",
    "@alcumus/express-middlewares": "^0.0.6",
    "@alcumus/mongoose-lib": "^0.0.5",
    "@alcumus/platform-extractors": "^0.0.5",
    "@alcumus/response-utils": "^0.0.5",
    "@alcumus/service-mesh-plugin": "^0.0.6",
    "copyfiles": "^2.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/mongoose": "^5.10.5",
    "@types/morgan": "^1.9.2",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "debug": "^4.3.1",
    "jest": "26.6.0",
    "nodemon": "^1.19.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
